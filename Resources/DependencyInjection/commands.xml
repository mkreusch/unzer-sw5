<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="unzer_payment.commands.send_shipping" class="UnzerPayment\Commands\SendShippingCommand">
            <argument type="service" id="unzer_payment.services.config_reader"/>
            <argument type="service" id="unzer_payment.services.api_logger"/>
            <argument type="service" id="dbal_connection"/>

            <tag name="console.command"/>
        </service>
    </services>
</container>
